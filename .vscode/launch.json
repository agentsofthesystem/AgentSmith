    "version": "0.2.0",
    "configurations": [
        {
            "name": "Script: Run XBRL",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run_xbrl.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/scripts"
        },
        {
            "name": "Script: Run Indexer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run_indexer.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/scripts",
            "subProcess": true,
            "justMyCode": false
        },
        {
            "name": "Script: Run Downloader",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run_single_download.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/scripts",
            "subProcess": true,
            "justMyCode": false
        },
        {
            "name": "Script: Run YFinance",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run_yfinance.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/scripts"
        },
        {
            "name": "Script: Run Visualizer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run_visualization.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/scripts"
        },
        {
            "name": "Python: Celery",
            "type": "python",
            "module": "celery",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "worker",
                "-A",
                "application.wsgi_worker:WORKER",
                "--loglevel=INFO",
            ],
        }
    ]
}